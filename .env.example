# TTS Generator Environment Variables
# Copy this file to .env and fill in your actual values

# Get this from your browser's developer tools -> Application -> Cookies -> sessionid
TIKTOK_SESSION_ID=your_session_id_here

# Optional: Default voice code
DEFAULT_VOICE_CODE=en_us_002

# Optional: API Configuration
TTS_API_BASE_URL=https://api16-normal-v6.tiktokv.com/media/api/text/speech/invoke

# Optional: Performance Settings
MAX_CHUNK_LENGTH=200
MAX_WORKERS=8
RATE_LIMIT_DELAY=50
MAX_CACHE_SIZE=1000